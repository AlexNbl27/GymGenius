<Window x:Class="GymGenius.SplashWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GymGenius"
        xmlns:properties="clr-namespace:GymGenius.Properties"
        Background="#FFFFFF"
        mc:Ignorable="d"
        Title="GymGenius" Height="600" Width="800" WindowStartupLocation="CenterScreen">
    <Grid>
        <Viewbox>
            <Image Source="{DynamicResource Logo}" Height="550" Width="750" />
        </Viewbox>
    </Grid>
</Window>
